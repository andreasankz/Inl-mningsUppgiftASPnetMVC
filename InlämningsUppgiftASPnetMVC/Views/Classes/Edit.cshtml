@model InlämningsUppgiftASPnetMVC.Entities.SchoolClass

@{
    ViewData["Title"] = "Edit";
}
<div class="container">
    <h1 class="mt-2">Edit</h1>

    <h4>SchoolClass</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Edit">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="Id" />
                <div class="form-group mt-2">
                    <label asp-for="ClassName" class="control-label"></label>
                    <input asp-for="ClassName" class="form-control" />
                    <span asp-validation-for="ClassName" class="text-danger"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="TeacherId" class="control-label"></label>
                    <input asp-for="TeacherId" class="form-control" />
                    <span asp-validation-for="TeacherId" class="text-danger"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="Created" class="control-label"></label>
                    <input asp-for="Created" class="form-control" />
                    <span asp-validation-for="Created" class="text-danger"></span>
                </div>
               <div class="form-group mt-4">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>

    <div class="mt-2">
        <a   asp-action="Index">Back to List</a>
    </div>

    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }

</div>
